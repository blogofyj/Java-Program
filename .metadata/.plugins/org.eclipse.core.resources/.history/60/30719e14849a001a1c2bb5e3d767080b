package ReviewJava;

public class First {

	public static void main(String[] args) {
		int[] array = new int[5];
		for (int i = 0; i < array.length; i++) {
			array[i] = i;
		}
		
		int[] ss = new int[] {1,2,3,4};
		for (int i : ss) {
			System.out.println(ss[i]);
		}
		
		System.out.println(array[4]);
	}
}
